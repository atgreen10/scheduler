<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="303.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="245.0" prefWidth="455.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="loginButton" layoutX="296.0" layoutY="230.0" mnemonicParsing="false" onAction="#onClickLoginButton" prefHeight="33.0" prefWidth="62.0" text="Login" />
            <TextField fx:id="userNameInput" layoutX="147.0" layoutY="119.0" promptText="Username" />
            <PasswordField fx:id="loginPasswordInput" layoutX="147.0" layoutY="173.0" promptText="Password" />
            <Label layoutX="91.0" layoutY="21.0" prefHeight="63.0" prefWidth="273.0" text="Appointment Scheduler Login" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
